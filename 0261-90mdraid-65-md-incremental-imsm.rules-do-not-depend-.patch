From d3b8373f4a4a262ef5d8900b1e8235e6e099ec66 Mon Sep 17 00:00:00 2001
From: Harald Hoyer <harald@redhat.com>
Date: Tue, 14 Feb 2012 14:07:58 +0100
Subject: [PATCH] 90mdraid/65-md-incremental-imsm.rules: do not depend on
 rd_MDADMCONF for timeout

---
 modules.d/90mdraid/65-md-incremental-imsm.rules | 2 --
 1 file changed, 2 deletions(-)

diff --git a/modules.d/90mdraid/65-md-incremental-imsm.rules b/modules.d/90mdraid/65-md-incremental-imsm.rules
index 06c5761..096f34d 100644
--- a/modules.d/90mdraid/65-md-incremental-imsm.rules
+++ b/modules.d/90mdraid/65-md-incremental-imsm.rules
@@ -40,7 +40,6 @@ ACTION=="add|change", \
 	KERNEL=="md[0-9]*|md/*", \
 	ENV{MD_LEVEL}!="container", \
 	ENV{MD_CONTAINER}!="?*", \
-        ENV{rd_MDADMCONF}!="?*", \
         ENV{rd_NO_MD}!="?*", \
 	GOTO="do_raidstart"
 
@@ -62,7 +61,6 @@ ACTION=="add|change", \
 	KERNEL=="md[0-9]*|md/*", \
 	ENV{DEVTYPE}!="partition", \
 	ENV{MD_LEVEL}=="container", \
-        ENV{rd_MDADMCONF}!="?*", \
         ENV{rd_NO_MD}!="?*", \
 	GOTO="do_container"
 
