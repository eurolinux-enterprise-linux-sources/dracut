From 936bd1e05fbd609fa6231d3fe55ef5550fee9391 Mon Sep 17 00:00:00 2001
From: Harald Hoyer <harald@redhat.com>
Date: Fri, 8 May 2015 13:49:57 +0200
Subject: [PATCH] iscsi: start iscsi only for iscsi netroot

---
 modules.d/95iscsi/iscsiroot | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/modules.d/95iscsi/iscsiroot b/modules.d/95iscsi/iscsiroot
index 671c56a..87fcaf6 100755
--- a/modules.d/95iscsi/iscsiroot
+++ b/modules.d/95iscsi/iscsiroot
@@ -178,7 +178,7 @@ handle_netroot()
 # loop over all netroot parameter
 if getarg netroot; then
     for nroot in $(getargs netroot); do
-        [ "${netroot%%:*}" = "iscsi" ] || continue
+        [ "${nroot%%:*}" = "iscsi" ] || continue
 	handle_netroot ${nroot##iscsi:}
     done
 else
