From 1b8636aa8e78746b541a7e936983dd05d0eeb83f Mon Sep 17 00:00:00 2001
From: Harald Hoyer <harald@redhat.com>
Date: Fri, 19 Mar 2010 15:08:54 +0100
Subject: [PATCH] lvm/lvm_scan.sh: silence lvm version check

---
 modules.d/90lvm/lvm_scan.sh | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/modules.d/90lvm/lvm_scan.sh b/modules.d/90lvm/lvm_scan.sh
index f4f7b4a..784329e 100755
--- a/modules.d/90lvm/lvm_scan.sh
+++ b/modules.d/90lvm/lvm_scan.sh
@@ -55,7 +55,7 @@ nopoll=$(
 	maj=${maj##*:}; 
 	sub=${sub%% *}; sub=${sub%%\(*}; 
 	check_lvm_ver $maj $min $sub && \
-	    echo " --poll n "))
+	    echo " --poll n ") 2>/dev/null )
 
 if [ -n "$LVS" ] ; then
     info "Scanning devices $lvmdevs for LVM logical volumes $LVS"
